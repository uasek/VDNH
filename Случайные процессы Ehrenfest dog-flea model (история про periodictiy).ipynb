    "V[-10:, :].round(3)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Сценарий 2: $X_0$ равномерно распредлен на $\\{0, 1, ... m\\}$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "m = 5\n",
    "s_distr = np.ones(m + 1) / (m + 1)\n",
    "t_5, V_5, B_5 = distr(s_distr, 10 ** 3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "m = 6\n",
    "s_distr = np.ones(m + 1) / (m + 1)\n",
    "t_6, V_6, B_6 = distr(s_distr, 10 ** 3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
