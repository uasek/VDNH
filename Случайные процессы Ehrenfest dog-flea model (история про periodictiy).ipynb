   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "m = 5\n",
    "s_distr = stats.binom.pmf(np.arange(m + 1), m, 0.5)\n",
    "t, V, B = distr(s_distr, 10 ** 3)"
   ]
